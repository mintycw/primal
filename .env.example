# Client-side variables (exposed to browser)
NEXT_PUBLIC_API_BASE_URL=localhost:3000

# Server-side variables (not exposed to browser)
MONGODB_URI=mongodb:://username:password@host:port/dbname?authSource=authdb
S3_ENDPOINT=http://localhost:3000
S3_BUCKET=storage-name
S3_REGION=eu-central-1
S3_ACCESS_KEY=
S3_SECRET_KEY=

# Enable or disable video compression
VIDEO_COMPRESSION=false
VIDEO_COMPRESSION_ENDPOINT=

# For local compression. NOT RECOMMENDED:
LOCAL_VIDEO_COMPRESSION=false
LOCAL_VIDEO_CODEC=hevc_videotoolbox